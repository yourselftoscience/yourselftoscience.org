{
  "name": "yourselftoscience.org",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:next": "next dev",
    "dev:watch": "node scripts/watchUpdatePdf.js",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:watch\"",
    "prebuild": "npm run update-sitemap && npm run update-csv && npm run update-json && npm run update-markdown || echo 'Prebuild scripts failed, continuing build'",
    "build": "next build",
    "postbuild": "node scripts/updatePdfPage.js || echo 'PDF generation failed, continuing deployment'",
    "deploy": "next-on-pages",
    "start": "next start",
    "test": "node --test",
    "lint": "next lint",
    "watch-pdf": "node scripts/watchUpdatePdf.js",
    "update-sitemap": "node scripts/generateSitemap.js",
    "update-csv": "node scripts/generateResourceCSV.js",
    "update-json": "node scripts/generateResourceJSON.js",
    "update-markdown": "node scripts/generateMarkdown.js"
  },
  "browserslist": [
    "extends browserslist-config-baseline"
  ],
  "dependencies": {
    "@cloudflare/pages-plugin-static-forms": "^1.0.3",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@headlessui/react": "^2.2.2",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@vercel/routing-utils": "^3.1.0",
    "browserslist-config-baseline": "^0.4.0",
    "chokidar": "^3.5.3",
    "debug": "^4.3.7",
    "framer-motion": "^11.11.1",
    "glob": "^11.0.0",
    "next": "^15.2.4",
    "node-fetch": "^2.6.7",
    "puppeteer": "^24.4.0",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-select": "^5.8.1",
    "resend": "^4.6.0",
    "rimraf": "^6.0.1",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/fun": "^1.1.1",
    "@vercel/node": "^3.2.23",
    "@vercel/redwood": "^2.1.8",
    "@vercel/static-build": "^1.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "editorconfig-tools": "^0.1.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.2.4",
    "globstar": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tar": "^7.4.3",
    "typescript": "^5",
    "vercel": "^37.11.0",
    "wrangler": "^3.28.2"
  }
}
